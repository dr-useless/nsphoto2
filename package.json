{
  "name": "nsphoto2",
  "version": "1.0.0",
  "description": "Website for Nicole Schafer Photo & Film, built with Hugo & netlifycms",
  "repository": "dr-useless/nsphoto2",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:hugo": "env HUGO_BASEURL=\"/\" hugo -d ../dist -s site -vw --",
    "start:esbuild": "node ./build.js --watch",
    "start:server": "node ./dev-server.js",
    "prebuild": "rimraf dist",
    "build": "npm run build:esbuild && npm run build:hugo",
    "build:hugo": "hugo -d ../dist -s site -v",
    "build:esbuild": "node ./build.js"
  },
  "author": "dr-useless",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.12.5",
    "hugo-bin": "^0.71.1",
    "netlify-cms-app": "^2.15.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "typescript": "^4.3.2",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "date-fns": "^1.30.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "express": "^4.17.1"
  },
  "dependencies": {}
}
