{
  "name": "nsphoto2",
  "version": "1.0.0",
  "description": "Website for Nicole Schafer Photo & Film, built with Hugo & netlifycms",
  "repository": "dr-useless/nsphoto2",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:hugo": "env HUGO_BASEURL=\"/\" hugo -d ../dist -s site -vw --",
    "start:parcel": "parcel src/index.js src/cms.html",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:parcel": "npm run start:parcel",
    "prebuild": "rimraf dist",
    "build": "npm run build:parcel && npm run build:hugo",
    "build:preview": "npm run build:parcel && npm run build:hugo:preview",
    "build:hugo": "hugo -d ../dist -s site -v",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:parcel": "parcel build src/index.js src/cms.html"
  },
  "author": "dr-useless",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "date-fns": "^1.30.1",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "hugo-bin": "^0.71.1",
    "netlify-cms-app": "^2.15.11",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^3.0.0",
    "typescript": "^4.3.2",
    "express": "^4.17.1"
  },
  "dependencies": {}
}
