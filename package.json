{
  "name": "nsphoto2",
  "version": "1.0.0",
  "description": "Website for Nicole Schafer Photo & Film, built with Hugo & netlifycms",
  "repository": "dr-useless/nsphoto2",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:hugo": "hugo -d ../dist -s site --watch",
    "start:esbuild": "node ./build.js --dev",
    "start:server": "node ./dev-server.js",
    "prebuild": "rimraf dist",
    "build": "npm run build:hugo && npm run build:esbuild",
    "build:hugo": "hugo -d ../dist -s site --minify",
    "build:esbuild": "node ./build.js"
  },
  "author": "dr-useless",
  "license": "MIT",
  "dependencies": {
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "date-fns": "^2.22.1",
    "esbuild": "^0.12.5",
    "hugo-bin": "^0.71.1",
    "netlify-cms-app": "^2.15.17",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "connect-livereload": "^0.6.1",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.4",
    "express": "^4.17.1",
    "livereload": "^0.9.3"
  }
}
